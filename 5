#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int x;
    cin>>x;
    setlocale(LC_ALL, " ");
    if (x!=0)
    {
        if (x>0)
    {
      cout<<"положительное"<<" ";
    } else
    {
        cout<<"отрицательное"<<" ";
    }
    if (x%2!=0)
    {
      cout<<"нечетное число";
    } else
    {
        cout<<"четное число";
    }
    } else
    {
      cout<<"нулевое число";
    } 
    return 0;
}
