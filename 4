#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int x, y;
    cin>>x>>y;
    setlocale(LC_ALL, " ");
    if (x>0 && y>0)
    {
      cout<<"1 четверть";
    } 
    if (x<0 && y>0)
    {
      cout<<"2 четверть";
    } 
    if (x<0 && y<0)
    {
      cout<<"3 четверть";
    } 
    if (x>0 && y<0)
    {
      cout<<"4 четверть";
    } 
    return 0;
}
